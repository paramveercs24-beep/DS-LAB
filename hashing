#include <stdio.h>

#define SIZE 10

int hashTable[SIZE];

void insert(int key)
{
    int index = key % SIZE;

    while (hashTable[index] != -1)
    {
        index = (index + 1) % SIZE;   // Linear probing
    }

    hashTable[index] = key;
}

void display()
{
    int i;
    printf("\nHash Table:\n");
    for (i = 0; i < SIZE; i++)
    {
        if (hashTable[i] != -1)
            printf("Index %d : %d\n", i, hashTable[i]);
        else
            printf("Index %d : Empty\n", i);
    }
}

int main()
{
    int i, n, key;

    for (i = 0; i < SIZE; i++)
        hashTable[i] = -1;

    printf("Enter number of employee records: ");
    scanf("%d", &n);

    printf("Enter 4-digit employee keys:\n");
    for (i = 0; i < n; i++)
    {
        scanf("%d", &key);
        insert(key);
    }

    display();

    return 0;
}
